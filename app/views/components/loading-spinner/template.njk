<div class="app-c-loading-spinner {{- ' ' + params.classes if params.classes }}" role="progressbar" aria-label="Loading, please wait." tabindex="0" {%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="-50 -50 100 100">
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(0, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-0 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(30, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-1 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(60, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-2 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(90, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-3 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(120, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-4 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(150, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-5 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(180, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-6 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(210, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-7 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(240, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-8 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(270, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-9 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(300, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-10 1s linear 0s infinite normal none running;"></rect>
      <rect fill="#000" width="12" height="5" rx="2.5" ry="2.5" transform="rotate(330, 0, 2) translate(10 0)" opacity="0.25" style="animation: app-loading-spinner-opacity-11 1s linear 0s infinite normal none running;"></rect>
  </svg>
</div>
