3eApplications by region and country\u003c/h2\u003e\n\n\u003ctable\u003e\n  \u003cthead\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"col\"\u003eRegion/country\u003c/th\u003e\n      \u003cth scope=\"col\"\u003eAugust applications\u003c/th\u003e\n      \u003cth scope=\"col\"\u003eSeptember applications\u003c/th\u003e\n      \u003cth scope=\"col\"\u003eOctober applications\u003c/th\u003e\n    \u003c/tr\u003e\n  \u003c/thead\u003e\n  \u003ctbody\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eSouth East\u003c/th\u003e\n      \u003ctd\u003e395,716\u003c/td\u003e\n      \u003ctd\u003e387,778\u003c/td\u003e\n      \u003ctd\u003e430,303\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eGreater London\u003c/th\u003e\n      \u003ctd\u003e316,939\u003c/td\u003e\n      \u003ctd\u003e318,300\u003c/td\u003e\n      \u003ctd\u003e347,443\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eNorth West\u003c/th\u003e\n      \u003ctd\u003e196,381\u003c/td\u003e\n      \u003ctd\u003e194,171\u003c/td\u003e\n      \u003ctd\u003e227,567\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eSouth West\u003c/th\u003e\n      \u003ctd\u003e164,696\u003c/td\u003e\n      \u003ctd\u003e162,681\u003c/td\u003e\n      \u003ctd\u003e181,312\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eWest Midlands\u003c/th\u003e\n      \u003ctd\u003e146,051\u003c/td\u003e\n      \u003ctd\u003e153,930\u003c/td\u003e\n      \u003ctd\u003e168,325\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eYorkshire and the Humber\u003c/th\u003e\n      \u003ctd\u003e133,111\u003c/td\u003e\n      \u003ctd\u003e135,492\u003c/td\u003e\n      \u003ctd\u003e152,736\u003c/td\u003e\n    \u003c/tr\u003e\n    \u003ctr\u003e\n      \u003cth scope=\"row\"\u003eEast Midlands\u003c/th\u003e\n      \u003ctd\u003e122,250\u003c